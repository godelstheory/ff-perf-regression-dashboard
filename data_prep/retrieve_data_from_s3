
#!/usr/bin/env bash

# Retrieve and clean RelDS quantiles
echo 'Retrieving RelDS quantiles from s3'
rm ../data/relds_quantiles/*
aws s3 cp s3://net-mozaws-prod-us-west-2-pipeline-analysis/cdowhygelund/CRT/V0_15_full/page_load/relds_csv/ ../data/relds_quantiles --recursive --quiet
rm ../data/relds_quantiles/_*
rm ../data/relds_quantiles/part-00000-*

# Retrieve and clean means of client histograms
echo 'Retrieving client means from s3'
aws s3 cp s3://net-mozaws-prod-us-west-2-pipeline-analysis/cdowhygelund/CRT/V0_15_full/page_load/client_means/ ../data/client_means --recursive --quiet
rm ../data/client_means/_*
rm ../data/client_means/part-00000-*